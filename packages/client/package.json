		"@babel/preset-env": "7.13.8",
		"@babel/preset-react": "7.12.13",
		"@babel/preset-typescript": "7.13.0",
		"@testing-library/jest-dom": "5.11.9",
		"@testing-library/react": "11.2.5",
		"@types/history": "4.7.8",
		"@types/html-webpack-plugin": "3.2.4",
		"@types/react": "16.14.4",
